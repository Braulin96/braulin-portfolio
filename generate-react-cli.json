{
  "usesTypeScript": false,
  "usesStyledComponents": false,
  "usesCssModule": true,
  "cssPreprocessor": "scss",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "customTemplates": {
        "component": "grc/component/TemplateName.tsx",
        "test": "grc/component/TemplateName.test.tsx",
        "style": "grc/component/TemplateName.styles.scss"
      },
      "path": "src/components",
      "withStyle": true,
      "withTest": true,
      "withStory": false,
      "withLazy": false
    },
    "section": {
      "customTemplates": {
        "component": "grc/section/TemplateName.tsx",
        "style": "grc/section/TemplateName.styles.scss"
      },
      "path": "src/sections",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    }
  }
}
